    "@babel/plugin-transform-typescript": "^7.21.3",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-dom": "^3.2.47",
    "esno": "^0.16.3",
    "kolorist": "^1.7.0",
